Package: MEIGOR
Type: Package
Title: MEIGOR - MEtaheuristics for bIoinformatics Global Optimization
Version: 1.36.0
Date: 2020-06-09
Authors@R:c(
    person("Jose A.", "Egea", email = "josea.egea@gmail.com", role = c("aut", "cre")),
    person("David", "Henriques", role = "aut"),
    person("Alexandre", "Fdez. Villaverde", role = "aut"),
